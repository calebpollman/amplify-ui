{
  "name": "react-native-example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "dev": "react-native start",
    "ios": "react-native run-ios",
    "ios:pod-install": "pod install --project-directory=./ios",
    "ios:build:storybook": "yarn prestorybook && yarn ios --scheme 'Storybook'",
    "ios:build:test": "yarn ios --scheme 'Test'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "prestorybook": "rnstl",
    "test": "jest"
  },
  "dependencies": {
    "aws-amplify": "in-app-messaging",
    "@aws-amplify/ui": "*",
    "@aws-amplify/ui-react-core": "*",
    "@aws-amplify/ui-react-native": "*",
    "@react-native-async-storage/async-storage": "^1.15.14",
    "@react-native-community/netinfo": "^7.1.7",
    "amazon-cognito-identity-js": "^5.2.3",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-config": "^1.4.5",
    "react-native-get-random-values": "^1.7.1",
    "react-native-url-polyfill": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/react-native": "^5.3.25",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-native-storybook-loader": "~2.0.5",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": [
        "./storybook"
      ],
      "pattern": "**/*.stories.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
